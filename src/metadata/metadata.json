          "required": true
        },
        {
          "name": "functionName",
          "type": "String",
          "info": "Name of the Lambda function",
          "required": true
        },
        {
          "name": "eventSourceArn",
          "type": "String",
          "info": "The Amazon Resource Name (ARN) of the Amazon Kinesis or the Amazon DynamoDB stream that is the event source.",
          "required": true
        },
        {
          "name": "startingPosition",
          "type": "Select",
          "options": [
            "TRIM_HORIZON",
            "LATEST"
          ],
          "info": "Alias of the Lambda function",
          "required": true
        },
        {
          "name": "enabled",
          "type": "Select",
          "options": [
            "true",
            "false"
          ],
          "info": "Indicates whether AWS Lambda should begin polling the event source. By default, Enabled is true.",
          "required": false
        },
        {
          "name": "batchSize",
          "type": "Number",
          "info": "The largest number of records that AWS Lambda will retrieve from your event source at the time of invoking your function. Your function receives an event with all the retrieved records. The default is 100 records.",
